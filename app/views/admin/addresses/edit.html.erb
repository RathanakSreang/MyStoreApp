<div class="row">
  <div class="col-md-6 col-md-offset-3 well">
    <legend>Store Address</legend>
    <%= form_for [:admin, @address], validate: true do |f| %>    
      <%= render "addresses/form", f: f %>  
      <div class="form-group">
        <%= f.submit "Update", class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>
</div>
