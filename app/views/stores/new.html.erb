<div class="row">
  <div class="col-md-4 col-md-offset-4 well well-sm">
    <legend>Create New Store</legend>    
    <%= form_for @store, validate: true do |f| %>
      <%= render "form", f: f %>
      <div class="form-group">
        <%= f.submit "Create", class: "btn btn-primary btn-block" %>
      </div>
    <% end %>
  </div>
</div>
